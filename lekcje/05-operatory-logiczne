# czy jest to liczba parzysta?
c = 4
print (c % 2 ==0)

# czy jest liczbą nieparzystą?
c = 5
print(not c % 2 ==0)

# czy użytkownik zalogowany?
user_logged_in = False

if user_logged_in:
    print("Zalogowany")
if not user_logged_in:
    print("Niezalogowany")

